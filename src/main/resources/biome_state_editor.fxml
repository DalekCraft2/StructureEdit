<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.dalekcraft.structureedit.ui.editor.BiomeStateEditorController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints vgrow="ALWAYS" />
   </rowConstraints>
   <children>
      <Label text="%ui.editor.biomestate.biomestate_id" />
      <TextField fx:id="biomeStateIdTextField" centerShape="false" disable="true" GridPane.columnIndex="1">
         <tooltip>
            <Tooltip text="%ui.editor.biomestate.biomestate_id.tooltip" />
         </tooltip>
      </TextField>
      <ListView fx:id="biomeStateListView" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="1" />
   </children>
</GridPane>
